{
    "name": "johnny13/phppaint",
    "description": "Generate Images from a given color palette",
    "keywords": ["phpPAINT", "Automated Art", "Dynamic Designs"],
    "license": "MIT",
    "type": "project",
    "homepage": "https://huement.com/phpPAINT",
    "authors": [{
        "name": "Johnny 13",
        "email": "admin@huement.com"
    }],
    "require": {
        "php": "^7.1.3",
        "claviska/simpleimage": "^3.3",
        "dallgoot/yaml": "^0.3.2",
        "docopt/docopt": "^1.0",
        "imagine/imagine": "^1.2",
        "intervention/image": "^2.5",
        "kevinlebrun/colors.php": "^1.0",
        "lastguest/pixeler": "^1.1",
        "league/climate": "^3.5",
        "league/color-extractor": "0.3.*",
        "spatie/browsershot": "^3.37"
    },
    "autoload": {
        "psr-4": {
            "Art\\": "src/Art/"
        },
        "minimum-stability": "dev"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
